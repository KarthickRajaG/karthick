import java.util.Scanner;

public class logical1 {
public static void main(String[] args) {
	int large=0;
	int small=0;
	Scanner scr=new Scanner(System.in);
	int n=scr.nextInt();
	
	for(int i=1;i<=n;i++)
	{
		int num=scr.nextInt();
		if(num>large)
		{
			large=num;
		}
		System.out.println("large =" + large);
		}
	System.out.println(large);
	}
}

